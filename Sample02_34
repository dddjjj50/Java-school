package chap02;

public class Sample02_34 {

	public static void main(String[] args) {
		byte b1 = 1;
		int i1 = b1 << 1;
		
		short s1 = 1234;
		int i2 = s1 >>> 5;
		
		short s2 = -1234;
		int i3 = s2 >> 3;
		
		System.out.println("i1の値は" + i1 + "です。");
		System.out.println("i2の値は" + i2 + "です。");
		System.out.println("i3の値は" + i3 + "です。");
	}

}
