package chap02;

public class Sample02_21 {

	public static void main(String[] args) {
		int i=0;
		
		System.out.println(++i);
		//System.out.println(i);
		System.out.println(i--);
		System.out.println(i);
		System.out.println(--i);
		//System.out.println(i);
		System.out.println(i--);
		System.out.println(i);
		

	}

}
